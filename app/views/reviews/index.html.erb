<%= render partial: "layouts/top-header" %>
<div class="main">
  <div class="main-top">
    <div class="main-top-lists">
      <div class="main-top-lists-left">
        <a href="#">Latest Dounut</a>
      </div>
      <label>
        <div class="main-top-lists-navbar">
          <form class="main-top-lists-navbar-form" id="review_seach" action="/reviews/serch" accept-charset="UTF-8" method="get">
            <input name="utf8" type="hidden" value="✓">
            <input placeholder="Dounutsを検索" class="main-top-lists-navbar-form-serch" type="text" name="q[name_cont]" id="q_name_cont">
            <button name="button" type="submit" class="main-top-lists-navbar-form-button">
              <i class="fas fa-search main-top-lists-navbar-form-button-icon">::before</i>
        </div>
      </label>
    </div>
  </div>
  <div class="main-reviews">
    <div class="main-reviews-content">
      <div class="main-reviews-content-soundcloud">
      </div>
      <div class="main-reviews-content-infomaitions">
        <div class="main-reviews-content-infomations-top">
          <div class="main-reviews-content-infomations-icon">
          </div>
          <div class="main-reviews-content-infomations-user">
            <div class="main-reviews-content-infomations-user-name">
              <p>Keita</p>
            </div>
            <div class="main-reviews-content-infomations-user-acccount">
              <p>@keitatensai</p>
            </div>
            <div class="main-reviews-content-infomations-user-time">
              <p>・ 54m</p>
            </div>
          </div>
        </div>
        <div class="main-reviews-content-infomations-low">
          <div class="main-reviews-content-reactions">
            <div class="main-reviews-content-reactions-comment">
              <a href="#">
                <i class="far fa-comment"></i>
              </a>
            </div>
            <div class="main-reviews-content-reactions-choice">
              <a href="#">
                <i class="fas fa-record-vinyl"></i>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>